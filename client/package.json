{
  "name": "shri-hw-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "start-server:dev": "npm run clean-build-folders && npm run build && npm run build-server && npm run dev-server",
    "start-server:prod": "npm run clean-build-folders && npm run build && npm run build-server && npm run run-server",
    "build-server": "webpack --config webpack.server.js",
    "run-server": "node ./build-server/bundle.js",
    "dev-server": "nodemon ./build-server/bundle.js",
    "clean-build-folders": "rm -rf ./build/ && rm -rf ./build-server/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "ts-node",
    "moduleNameMapper": {
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^store(.*)$": "<rootDir>/src/store$1"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@bem-react/classname": "^1.5.8",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "ansi-to-html": "^0.6.14",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "css-loader": "^3.4.2",
    "date-fns": "^2.11.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "ignore-styles": "^5.0.1",
    "jest": "^25.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "prop-types": "^15.7.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
