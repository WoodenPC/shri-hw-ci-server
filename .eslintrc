  
{
  "extends": ["airbnb-base"],
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
  "rules": {
      "semi": "error",
      "linebreak-style": 0,
      "import/prefer-default-export": 0,
      "max-classes-per-file": 0,
      "consistent-return": 0,
      "no-plusplus": 0,
      "arrow-body-style": 0,
      "max-len": 0,
      "operator-linebreak": 0,
      "object-curly-newline": 0
  }
}